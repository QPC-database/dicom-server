{
  "version": "2.0",
  "tracing": { "consoleLevel": "verbose" },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "DicomFunctions": {
    "Reindex": {
      "BatchSize": 2,
      "ParallelBatches": 2
    }
  },
  "SqlServer": {
    "ConnectionString": "server=(local);Initial Catalog=Dicom;Integrated Security=true"
  }
}
